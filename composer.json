{
    "name": "cxlblm/php-ranker",
    "description": "a sorting tool that supports dense_rank/rank/row_number sorting",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cxlblm\\PhpRanker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/"
        }
    },
    "authors": [
        {
            "name": "cxlblm",
            "email": "cxlblm@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.0",
        "phpstan/phpstan": "^1.9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "keywords": [
        "rank",
        "dense",
        "dense_rank",
        "row_number"
    ]
}
